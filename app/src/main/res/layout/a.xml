                    android:textSize="@dimen/egbaa"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/spgpa"
                    android:inputType="numberDecimal"
                    android:layout_weight="1"
                    android:backgroundTint="?colorPrimaryDark"
                    android:gravity="center"></EditText>


            </LinearLayout>
        </RelativeLayout>

    </ScrollView>


    <LinearLayout
        android:id="@+id/lha"
        android:layout_below="@+id/scrollView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16sdp"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView"
            android:layout_width="@dimen/_52sdp"
            android:layout_height="@dimen/_26sdp"
            android:layout_below="@id/finish"
            android:layout_gravity="center"
            android:background="?colorPrimaryDark"
            android:gravity="center"
            android:text="Result"
            android:textSize="@dimen/_12sdp"
            android:textColor="@color/backbutt" />

        <TextView
            android:id="@+id/cong"
            android:layout_width="@dimen/_190sdp"
            android:layout_height="@dimen/_26sdp"
            android:layout_gravity="center"
            android:gravity="center"
            android:padding="0dp"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="@dimen/_18sdp"/>


    </LinearLayout>

    <Button
        android:layout_marginTop="@dimen/_12sdp"
        android:id="@+id/finish"
        android:layout_width="@dimen/_72sdp"
        android:layout_height="@dimen/_36sdp"
        android:layout_below="@id/lha"
        android:layout_centerHorizontal="true"
        android:background="@drawable/shape"
        android:text="Finish"
        android:textSize="@dimen/_12sdp"
        android:textColor="@color/backbutt"></Button>


</RelativeLayout>